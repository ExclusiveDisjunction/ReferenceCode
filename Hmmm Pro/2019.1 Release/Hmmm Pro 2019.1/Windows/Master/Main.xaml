<Window x:Class="HmPro.Windows.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HmPro.Windows"
        mc:Ignorable="d"
        Title="Hmmm Pro" Height="660" Width="1057" WindowStartupLocation="CenterScreen" FontFamily="Century Gothic" Loaded="Window_Loaded" KeyDown="Window_KeyDown" Closing="Window_Closing" MinHeight="660" MinWidth="1057">

    <Window.Resources>
        <Style TargetType="{x:Type Grid}">
            <Style.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Property="Opacity" Value="0.7"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="RibbonButton" TargetType="{x:Type Button}">
            <Setter Property="Background">
                <Setter.Value>
                    <SolidColorBrush Color="{StaticResource Accent1}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <SolidColorBrush Color="{DynamicResource Accent3}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="BorderThickness" Value="2"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="13"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Trigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation Duration="0:0:0.250" To="2.5" Storyboard.TargetProperty="BorderThickness"/>
                                <ColorAnimation Duration="0:0:0.250" To="#FFCB9343" Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"/>
                                <ColorAnimation Duration="0:0:0.255" To="#FFB86F07" Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation Duration="0:0:0.250" To="2" Storyboard.TargetProperty="BorderThickness" />
                                <ColorAnimation Duration="0:0:0.450" To="#FFB86F07" Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"/>
                                <ColorAnimation Duration="0:0:0.455" To="#FF7E571F" Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Property="Opacity" Value="0.7"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="RibbonHeader" TargetType="{x:Type TabItem}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="17"/>
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <SolidColorBrush Color="{DynamicResource Grey1}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Background">
                <Setter.Value>
                    <SolidColorBrush Color="{DynamicResource Accent1}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Border>
                            <Grid>
                                <Grid>
                                    <Border x:Name="border" BorderThickness="2,2,2,0" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}"/>
                                </Grid>
                                <ContentPresenter ContentSource="Header" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background">
                                    <Setter.Value>
                                        <SolidColorBrush Color="{DynamicResource Accent1}"/>
                                    </Setter.Value>
                                </Setter>
                                <Setter TargetName="border" Property="BorderBrush">
                                    <Setter.Value>
                                        <SolidColorBrush Color="{DynamicResource Grey1}"/>
                                    </Setter.Value>
                                </Setter>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="border" Property="Background">
                                    <Setter.Value>
                                        <SolidColorBrush Color="{DynamicResource Accent2}"/>
                                    </Setter.Value>
                                </Setter>
                                <Setter TargetName="border" Property="BorderBrush">
                                    <Setter.Value>
                                        <SolidColorBrush Color="{DynamicResource Grey1}"/>
                                    </Setter.Value>
                                </Setter>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Opacity" Value="0.7"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="MainAreaButton" TargetType="{x:Type Button}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontFamily" Value="SimSun"/>
            <Setter Property="FontSize" Value="15"/>
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <SolidColorBrush Color="{DynamicResource Grey1}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="BorderThickness" Value="2"/>
            <Setter Property="Background">
                <Setter.Value>
                    <SolidColorBrush Color="{DynamicResource Accent1}"/>
                </Setter.Value>
            </Setter>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Trigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation Duration="0:0:0.100" To="3" Storyboard.TargetProperty="BorderThickness"/>
                                <ColorAnimation Duration="0:0:0.300" To="#FF7E571F" Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"/>
                                <ColorAnimation Duration="0:0:0.320" To="#FF666666" Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation Duration="0:0:0.100" To="2" Storyboard.TargetProperty="BorderThickness"/>
                                <ColorAnimation Duration="0:0:0.300" To="#FFB86F07" Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"/>
                                <ColorAnimation Duration="0:0:0.320" To="#FF212121" Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Property="Opacity" Value="0.7"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="RoundAppendButtons" TargetType="{x:Type Button}">
            <Setter Property="BorderBrush" Value="{x:Null}"/>
            <Setter Property="Background" Value="{x:Null}"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border BorderThickness="2">
                            <Border.Style>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="BorderBrush">
                                        <Setter.Value>
                                            <SolidColorBrush Color="{DynamicResource Accent1}"/>
                                        </Setter.Value>
                                    </Setter>
                                    <Setter Property="Background" Value="#FF303030"/>
                                    <Setter Property="CornerRadius" Value="6"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="BorderBrush">
                                                <Setter.Value>
                                                    <SolidColorBrush Color="{DynamicResource Accent1}"/>
                                                </Setter.Value>
                                            </Setter>
                                            <Setter Property="Background">
                                                <Setter.Value>
                                                    <SolidColorBrush Color="{DynamicResource Accent1}"/>
                                                </Setter.Value>
                                            </Setter>
                                            <Setter Property="CornerRadius" Value="10"/>
                                        </Trigger>
                                        <Trigger Property="IsEnabled" Value="False">
                                            <Setter Property="Opacity" Value="0.7"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <LinearGradientBrush.RelativeTransform>
                    <TransformGroup>
                        <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                        <SkewTransform CenterY="0.5" CenterX="0.5"/>
                        <RotateTransform Angle="231.00900000000002" CenterY="0.5" CenterX="0.5"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </LinearGradientBrush.RelativeTransform>
                <GradientStop Color="{DynamicResource Background1}" Offset="0"/>
                <GradientStop Color="#FF7E571F" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>

        <!-- This grid contains the information for the Ribbon -->
        <Grid x:Name="RibbonGrid" Margin="0,0,0,475">
            <TabControl x:Name="Ribbon" Height="144" VerticalAlignment="Top" Margin="0,5,0,0">
                <TabControl.Background>
                    <SolidColorBrush Color="{DynamicResource Grey1}"/>
                </TabControl.Background>
                <TabControl.BorderBrush>
                    <SolidColorBrush Color="{DynamicResource Grey1}"/>
                </TabControl.BorderBrush>

                <TabItem x:Name="FileCmds" Header="File" Margin="5,-2,-44,-2" Style="{DynamicResource RibbonHeader}">
                    <Grid Margin="0,3,-3,0" Height="106" VerticalAlignment="Top">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.Background>
                            <SolidColorBrush Color="{DynamicResource Grey2}"/>
                        </Grid.Background>
                        <Grid x:Name="OpenCmd" Margin="9,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="OpenCmd_MouseDown">
                            <Button Content="Open" Click="OpenCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="65" Margin="13,0,14,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/File/Open.png"/>
                        </Grid>
                        <Grid x:Name="NewFileCmd" Margin="89,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="NewFileCmd_MouseDown">
                            <Button Content="New..." Click="NewSessionCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left"  Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="14,0,0,0" VerticalAlignment="Top" Width="48" Source="/Hmmm Pro;component/Graphics/Png/File/New.png"/>
                        </Grid>
                        <Grid x:Name="CloseFileCmd" Margin="169,5,0,0" HorizontalAlignment="Left" Width="81" Height="96" VerticalAlignment="Top" MouseDown="CloseFileCmd_MouseDown">
                            <Button Content="Close File" Click="CloseFileCmd_Click" Height="26" VerticalAlignment="Top" Width="81" Margin="0,70,0,0" HorizontalAlignment="Left"  Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="56" Margin="10,0,0,0" VerticalAlignment="Top" Width="57" Source="/Hmmm Pro;component/Graphics/Png/File/Close File.png"/>
                        </Grid>

                        <Grid x:Name="SaveCmd" Margin="261,10,0,0" HorizontalAlignment="Left" Width="75" Height="91" VerticalAlignment="Top" MouseDown="SaveCmd_MouseDown">
                            <Button Content="Save" Height="26" Click="SaveCmd_Click" VerticalAlignment="Top" Width="75" Margin="0,65,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="56" Margin="15,0,0,0" VerticalAlignment="Top" Width="48" Source="/Hmmm Pro;component/Graphics/Png/File/Save.png"/>
                        </Grid>
                        <Grid x:Name="SaveAsCmd" Margin="341,10,0,0" HorizontalAlignment="Left" Width="75" Height="91" VerticalAlignment="Top" MouseDown="SaveAsCmd_MouseDown">
                            <Button Content="Save As" Click="SaveAsCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,65,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="51" Margin="13,0,0,0" VerticalAlignment="Top" Width="48" Source="/Hmmm Pro;component/Graphics/Png/File/Save As.png"/>
                        </Grid>

                        <Grid x:Name="AppendCmd" Margin="427,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="AppendCmd_MouseDown">
                            <Button Content="Append" Height="26" Click="AppendCmd_Click" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="25,0,0,0" VerticalAlignment="Top" Width="30" Source="/Hmmm Pro;component/Graphics/Png/File/Append.png"/>
                        </Grid>
                        <Grid x:Name="AppendAllCmd" Margin="507,5,0,0" HorizontalAlignment="Left" Width="98" Height="96" VerticalAlignment="Top" MouseDown="AppendAllCmd_MouseDown">
                            <Button Content="Append All" Height="26" Click="AppendAllCmd_Click" VerticalAlignment="Top" Width="98" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="32,0,0,0" VerticalAlignment="Top" Width="32" Source="/Hmmm Pro;component/Graphics/Png/File/Append All.png"/>
                        </Grid>

                        <Grid x:Name="ExportCmd" Margin="696,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="ExportCmd_MouseDown">
                            <Button Content="Export..." Width="75" Click="ExportCmd_Click" Margin="0,70,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="60" Margin="9,5,0,0" VerticalAlignment="Top" Width="59" Source="/Hmmm Pro;component/Graphics/Png/File/Export.png"/>
                        </Grid>
                        <Grid x:Name="ImportCmd" Margin="618,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="ImportCmd_MouseDown">
                            <Button Content="Import..." Height="26" Click="ImportCmd_Click" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="54" Margin="8,6,0,0" VerticalAlignment="Top" Width="61" Source="/Hmmm Pro;component/Graphics/Png/File/Import.png"/>
                        </Grid>
                        <Grid x:Name="PropertiesCmd" Margin="795,5,0,0" HorizontalAlignment="Left" Width="83" Height="96" VerticalAlignment="Top" MouseDown="PropertiesCmd_MouseDown">
                            <Button Content="Properties..." Width="84" Click="PropertiesCmd_Click" Margin="0,70,-9,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="20,0,0,0" VerticalAlignment="Top" Width="48" Source="/Hmmm Pro;component/Graphics/Png/File/Properties.png"/>
                        </Grid>
                    </Grid>
                </TabItem>

                <TabItem x:Name="MemesCmds" Header="Memes" Margin="46,-2,-64,-2" Style="{DynamicResource RibbonHeader}">
                    <Grid Margin="0,3,-3,0" Height="106" VerticalAlignment="Top">
                        <Grid.Background>
                            <SolidColorBrush Color="{DynamicResource Grey2}"/>
                        </Grid.Background>

                        <Grid x:Name="AddMemeCmd" Margin="10,5,0,0" HorizontalAlignment="Left" Width="113" Height="96" VerticalAlignment="Top" MouseDown="AddMemeCmd_MouseDown">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="11*"/>
                                <ColumnDefinition Width="27*"/>
                            </Grid.ColumnDefinitions>
                            <Button Content="Add new Meme" Click="AddMemeCmd_Click" Height="26" VerticalAlignment="Top" Width="113" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}" Margin="0,70,0,0" Grid.ColumnSpan="2"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="24,4,0,0" VerticalAlignment="Top" Width="67" Source="/Hmmm Pro;component/Graphics/Png/Meme/Add Meme.png" Grid.ColumnSpan="2"/>
                        </Grid>
                        <Grid x:Name="ManageMemesCmd" Margin="128,5,0,0" HorizontalAlignment="Left" Width="118" Height="96" VerticalAlignment="Top" MouseDown="ManageMemesCmd_MouseDown">
                            <Button Content="Manage Memes" Click="ManageMemesCmd_Click" Height="26" VerticalAlignment="Top" Width="118"  Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="26,4,0,0" VerticalAlignment="Top" Width="67" Source="/Hmmm Pro;component/Graphics/Png/Meme/Manage Memes.png"/>
                        </Grid>
                        <Grid x:Name="ExportMemeCmd" Margin="251,5,0,0" HorizontalAlignment="Left" Width="120" Height="96" VerticalAlignment="Top" MouseDown="ExportMemesCmd_MouseDown">
                            <Button Content="Export Meme(s)" Click="ExportMemesCmd_Click" Height="26" VerticalAlignment="Top" Width="120" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="60" Margin="18,5,0,0" VerticalAlignment="Top" Width="86" Source="/Hmmm Pro;component/Graphics/Png/Meme/Export Meme.png"/>
                        </Grid>


                        <Grid x:Name="AddCollectionCmd" Margin="394,5,0,0" HorizontalAlignment="Left" Width="138" Height="96" VerticalAlignment="Top" MouseDown="AddCollectionCmd_MouseDown">
                            <Button Content="Add new Collection" Click="AddCollectionCmd_Click"  Height="26" VerticalAlignment="Top" Width="138" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="41,2,0,0" VerticalAlignment="Top" Width="60" Source="/Hmmm Pro;component/Graphics/Png/Meme/Add Collection.png"/>
                        </Grid>
                        <Grid x:Name="ManageCollectionsCmd" Margin="537,5,0,0" HorizontalAlignment="Left" Width="145" Height="96" VerticalAlignment="Top" MouseDown="ManageCollectionsCmd_MouseDown">
                            <Button Content="Manage Collections" Click="ManageCollectionsCmd_Click" Height="26" VerticalAlignment="Top" Width="145" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="38,2,0,0" VerticalAlignment="Top" Width="68" Source="/Hmmm Pro;component/Graphics/Png/Meme/Manage Collections.png"/>
                        </Grid>
                        <Grid x:Name="ExportCollectionCmd" Margin="687,5,0,0" HorizontalAlignment="Left" Width="139" Height="96" VerticalAlignment="Top" MouseDown="ExportCollectionsCmd_MouseDown">
                            <Button Content="Export Collection(s)" Click="ExportCollectionsCmd_Click"  Height="26" VerticalAlignment="Top" Width="139" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="60" Margin="31,5,0,0" VerticalAlignment="Top" Width="86" Source="/Hmmm Pro;component/Graphics/Png/Meme/Export Collection.png"/>
                        </Grid>

                        <Grid x:Name="MemeRegulationsCmd" Margin="845,5,0,0" HorizontalAlignment="Left" Width="135" Height="96" VerticalAlignment="Top" MouseDown="MemeRegulationsCmd_MouseDown" Visibility="Collapsed">
                            <Button Content="Meme Regulations..." Click="MemeRegulationsCmd_Click" Height="26" VerticalAlignment="Top" Width="135" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="60" Margin="36,5,0,0" VerticalAlignment="Top" Width="86" Source="/Hmmm Pro;component/Graphics/Png/Meme/Meme Regulations.png"/>
                        </Grid>
                    </Grid>
                </TabItem>

                <TabItem x:Name="EditCmds" Header="Edit" Margin="66,-2,-108,-2" Style="{DynamicResource RibbonHeader}">
                    <Grid Margin="0,3,-3,0" Height="106" VerticalAlignment="Top">
                        <Grid.Background>
                            <SolidColorBrush Color="{DynamicResource Grey2}"/>
                        </Grid.Background>

                        <Grid x:Name="AllObjectsCmd" Margin="10,5,0,0" HorizontalAlignment="Left" Width="119" Height="96" VerticalAlignment="Top" MouseDown="AllObjectsCmd_MouseDown">
                            <Button Content="All Objects" Click="AllObjectsCmd_Click" Height="26" VerticalAlignment="Top" Width="119" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="60" Margin="24,5,24,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Edit/All Objects.png"/>
                        </Grid>
                        <Grid x:Name="ManageCollectionsCmd1" Margin="134,5,0,0" HorizontalAlignment="Left" Width="135" Height="96" VerticalAlignment="Top" MouseDown="MemeRegulationsCmd_MouseDown">
                            <Button Content="Manage Collections" Click="ManageCollectionsCmd_Click" Height="26" VerticalAlignment="Top" Width="136" Margin="0,70,-9,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="65" Margin="19,0,29,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Meme/Manage Collections.png"/>
                        </Grid>
                        <Grid x:Name="ManageMemesCmd1" Margin="274,5,0,0" HorizontalAlignment="Left" Width="118" Height="96" VerticalAlignment="Top" MouseDown="ManageMemesCmd_MouseDown">
                            <Button Content="Manage Memes" Click="ManageMemesCmd_Click" Height="26" VerticalAlignment="Top" Width="118"  Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image HorizontalAlignment="Left" Height="65" Margin="26,4,0,0" VerticalAlignment="Top" Width="67" Source="/Hmmm Pro;component/Graphics/Png/Meme/Manage Memes.png"/>
                        </Grid>

                        <Grid x:Name="SettingsCmd" Margin="397,5,0,0" HorizontalAlignment="Left" Width="118" Height="96" VerticalAlignment="Top" MouseDown="SettingsCmd_MouseDown">
                            <Button Content="Settings" Click="SettingsCmd_Click" Height="26" VerticalAlignment="Top" Width="118"  Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="51" Margin="13,10,13,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Edit/Settings.png"/>
                        </Grid>

                        <Grid x:Name="ScriptEditorCmd" Margin="520,5,0,0" HorizontalAlignment="Left" Width="118" Height="96" VerticalAlignment="Top" MouseDown="ScriptEditorCmd_MouseDown">
                            <Button Content="Script Editor" Click="ScriptEditorCmd_Click" Height="26" VerticalAlignment="Top" Width="118"  Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="55" Margin="13,10,13,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Edit/Script Editor.png"/>
                        </Grid>
                    </Grid>
                </TabItem>

                <TabItem x:Name="WindowCmds" Header="Window" Margin="111,-2,-122,-2" Style="{DynamicResource RibbonHeader}">
                    <Grid Margin="0,3,-3,0" Height="106" VerticalAlignment="Top">
                        <Grid.Background>
                            <SolidColorBrush Color="{DynamicResource Grey2}"/>
                        </Grid.Background>

                        <Grid x:Name="MaximizeCmd" Margin="9,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="MaximizeCmd_MouseDown">
                            <Button Content="Maximize" Click="MaximizeCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="65" Margin="13,6,14,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Window/Maximize.png"/>
                        </Grid>
                        <Grid x:Name="MinimizeCmd" Margin="89,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="MinimizeCmd_MouseDown">
                            <Button Content="Minimize" Click="MinimizeCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="6" Margin="10,33,10,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Window/Minimize.png"/>
                        </Grid>
                        <Grid x:Name="NormalCmd" Margin="169,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="NormalCmd_MouseDown">
                            <Button Content="Normal" Click="NormalCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="65" Margin="13,6,14,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Window/Normal.png"/>
                        </Grid>
                        <Grid x:Name="AutoSetCmd" Margin="249,5,0,0" HorizontalAlignment="Left" Width="75" Height="96" VerticalAlignment="Top" MouseDown="AutoSizeCmd_MouseDown">
                            <Button Content="Auto Size" Click="AutoSizeCmd_Click" Height="26" VerticalAlignment="Top" Width="75" Margin="0,70,0,0" HorizontalAlignment="Left" Style="{DynamicResource RibbonButton}"/>
                            <Image Height="55" Margin="9,2,8,0" VerticalAlignment="Top" Source="/Hmmm Pro;component/Graphics/Png/Window/Auto Size.png"/>
                        </Grid>
